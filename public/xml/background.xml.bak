<bg>	
	
	<navi>
		
		<color>0x666666</color>		<!-- NORMAL COLOR OF THE NAVIGATION BULLETS -->
		<color>0x9F0000</color>		<!-- ROLLOVER COLOR OF THE NAVIGATION BULLETS -->
		
		<x>-5</x>					<!-- THE NAVIGATION POSITIONING ON X AXIS FORM THE RIGHT SIDE OF THE SCREEN -->
		<y>15</y>					<!-- THE NAVIGATION POSITIONING ON Y AXIS -->
		<spacer>5</spacer>			<!-- SPACE BETWEEN THE NAVIGATION BULLETS -->
			
	</navi>

	<!-- //////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- THE ARRANGEMENT PART OF THE XML FILE. HERE YOU CHOOSE WHAT KIND OF CONTENT TO DISPLAY AND IN WHAT ORDER. -->
	<!-- //////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	
	<section>	
		<!--<navi>on</navi>-->				<!-- NAVIGATION BULLETS ON/OFF -->
		<autoplay>off</autoplay>		<!-- SLIDESHOW ON/OFF -->
		<id>2</id>
		<!--<id>6</id>-->
		<!--<id>5</id>-->					<!-- THE 5TH CONTENT WILL PLAY FIRST. REMEMBER! COUNTING STARTS WITH 0. -->
		<!--<id>0</id>-->					<!-- THE 1ST CONTENT WILL FOLLOW. -->
		<!--<id>3</id>-->				
		<!--<id>1</id>-->				
