		<effectalpha>0.5</effectalpha>										<!-- ALPHA OF THE PATTERN. 0 = INVISIBLE / 1 = FULL OPACITY -->
		<smoothing>1</smoothing>  											<!-- 0 = TRUE / 1 = FALSE -->		
	</item>	
	
