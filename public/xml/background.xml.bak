	<item>
		<type>image</type>							<!-- TYPE OF THE CONTENT FILE -->
		<delay>5</delay>							<!-- HOW MANY SECONDS WILL DISPLAY IT -->
		<path>background/01.jpg</path>				<!-- THE PATH TO THE BACKGROUND FILE. IT CAN BE IMAGE,SWF OR VIDEO. -->
		<resize>scale</resize>						<!-- THE SCALE VALUE WILL MAINTAIN ASPECT RATIO AND MAKE IT FULLSCREEN. CHOOSE VALUES LIKE SCALE/FIT/STRETCH/NONE -->
		<verticalalign>center</verticalalign>		<!-- TOP/CENTER/BOTTOM -->
		<horizontalalign>center</horizontalalign>	<!-- LEFT/CENTER/RIGHT -->
		<alpha>1</alpha>							<!-- 0 = INVISIBLE / 1 = FULL OPACITY -->
